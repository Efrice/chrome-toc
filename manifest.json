{
  "manifest_version": 3,
  "name": "TOC",
  "version": "1.0.0",
  "description": "Generate TOC for the current page.",
  "permissions": ["history"],
  "background": {
    "service_worker": "scripts/background.js"
  },
  "content_scripts": [
    {
      "css": ["styles/content.css"],
      "js": ["scripts/content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "48": "icons/48.png"
  }
}
